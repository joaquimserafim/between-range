    "values",
    "range",
    "int",
    "integers",
    "util"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/between-range/issues"
  },
  "homepage": "https://github.com/joaquimserafim/between-range",
  "devDependencies": {
    "code": "^1.4.0",
    "jscs": "^2.5.1",
    "jshint": "^2.8.0",
    "lab": "^5.10.0",
    "pre-commit": "^1.1.2"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ]
